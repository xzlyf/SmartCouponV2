<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape android:shape="oval">

            <size android:width="@dimen/dp_20" android:height="@dimen/dp_20" />
            <solid android:color="@color/custom_progress_orange_progress" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="oval">
            <size android:width="@dimen/dp_20" android:height="@dimen/dp_20"/>
            <solid android:color="@color/custom_progress_orange_progress_half"/>
        </shape>
    </item>
</selector>